{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User Get Unauthorized Response",
  "description": "Schema for unauthorized access when trying to get user details.",
  "required": [
    "code",
    "message"
  ],
  "properties": {
    "code": {
      "type": "integer",
